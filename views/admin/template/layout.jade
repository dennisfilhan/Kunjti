- var iE = parseInt(id==null?0:id);
- var iN = parseInt(no==null?0:no);
.accordion
	.accordion-group
		.accordion-heading
			a.accordion-toggle(data-toggle='collapse', data-parent= '#units', href='#accordion'+iE) 
				span.btn(data-toggle='tooltip', title='Urutan agenda')
					= (iE+1)
				| &nbsp;&mdash;&nbsp;
				block name 
				| &nbsp;&mdash;&nbsp;
				= (iN+1)
			span.pull-right(style='position:relative; top:-38px; right:10px;')
				a.btn.close(href='javascript:;') &times;
		.accordion-body.collapse(id= 'accordion'+iE, class= (iE==0?'in':''))
			.accordion-inner
				block content